API_BASE_URL=https://api.woohakdong.com
# 로컬 개발: 프록시를 통해 Same-Origin으로 요청 (쿠키 자동 전송)
NEXT_PUBLIC_API_BASE_URL=/api
# 프로덕션용
# NEXT_PUBLIC_API_BASE_URL=https://api.woohakdong.com

# Firebase Secret
NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=
NEXT_PUBLIC_FIREBASE_PROJECT_ID=
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
NEXT_PUBLIC_FIREBASE_APP_ID=
NEXT_PUBLIC_MEASUREMENT_ID=

# Sentry
NEXT_PUBLIC_SENTRY_DSN=

# Amplitude
NEXT_PUBLIC_AMPLITUDE_API_KEY=

# PortOne (포트원)
# https://admin.portone.io 에서 발급
# 문서: https://developers.portone.io
#
# Store ID (상점 ID) - 클라이언트에서 사용
NEXT_PUBLIC_PORTONE_STORE_ID=
#
# Channel Keys (채널 키) - PG사별 채널 키
# KG이니시스 카드 정기결제 채널
NEXT_PUBLIC_PORTONE_CHANNEL_INICIS_BILLING=
# 카카오페이 정기결제 채널
NEXT_PUBLIC_PORTONE_CHANNEL_KAKAOPAY_BILLING=
#
# API Secret (서버에서만 사용)
# 관리자 콘솔 > 결제 연동 > API 키 에서 발급
PORTONE_API_SECRET=

# 테스트용 빌링 주기 (분 단위, 선택)
# 설정 시 구독 생성 시 endDate가 해당 분 후로 설정됨 (예: 3 = 3분)
# 주의: 스케줄러의 TEST_BILLING_CYCLE_MINUTES와 동일하게 설정해야 함
# NEXT_PUBLIC_TEST_BILLING_CYCLE_MINUTES=3
